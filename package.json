{
  "name": "vue-bundle-renderer",
  "version": "2.2.0",
  "description": "Bundle Renderer for Vue 3.0",
  "repository": "nuxt-contrib/vue-bundle-renderer",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "runtime.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint src",
    "prepack": "unbuild",
    "test": "pnpm lint && pnpm vitest run --coverage && tsc --noEmit",
    "bench": "vitest bench"
  },
  "dependencies": {
    "ufo": "^1.6.3"
  },
  "devDependencies": {
    "@codspeed/vitest-plugin": "^5.2.0",
    "@nuxt/eslint-config": "^1.15.1",
    "@types/node": "^24.10.13",
    "@types/semver": "^7.7.1",
    "@vitest/coverage-v8": "^4.0.18",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "pkg-pr-new": "^0.0.60",
    "semver": "^7.7.4",
    "std-env": "^3.10.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1",
    "vitest": "4.0.18",
    "vue": "3.5.22"
  },
  "packageManager": "pnpm@10.21.0"
}
